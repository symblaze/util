{
  "name": "symblaze/util",
  "description": "Symblaze php utilities.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "imdhemy",
      "email": "imdhemy@gmail.com"
    }
  ],
  "scripts": {
    "test": "phpunit --testdox",
    "cs-check": "vendor/bin/php-cs-fixer fix --dry-run --diff --verbose",
    "cs-fix": "php-cs-fixer fix --verbose",
    "psalm": "psalm --no-cache"
  },
  "autoload": {
    "psr-4": {
      "Symblaze\\Util\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Symblaze\\Util\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0|^10.0",
    "friendsofphp/php-cs-fixer": "^3.51",
    "vimeo/psalm": "^5.23"
  }
}
